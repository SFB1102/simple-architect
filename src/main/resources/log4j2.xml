<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="de.saar.coli.minecraft.MinecraftRealizer" level="info">
    </Logger>
    <Logger name="io.grpc.netty" level="info">
    </Logger>
    <Logger name="de.saar.minecraft.architect.ArchitectServer" level="info">
    </Logger>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
